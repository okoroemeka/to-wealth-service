{
  "name": "server-template",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/okoroemeka/server-template.git",
  "author": "okoroemeka <solomonokoro056@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "clean": "rimraf dist",
    "dev": "npm-run-all --parallel clean migrate",
    "dev:start": "cross-env NODE_ENV=development nodemon --exec babel-node src/server",
    "start": "pm2-runtime start ecosystem.config.js",
    "build": "babel ./src -d dist",
    "migrate": "sequelize db:migrate",
    "undo:migrate": "sequelize db:migrate:undo:all",
    "postinstall": "npm run dev"
  },
  "dependencies": {
    "@babel/register": "^7.8.6",
    "@hapi/joi": "^17.1.1",
    "bcrypt": "^4.0.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.9.1",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.3.0",
    "pm2": "^4.4.1",
    "sequelize": "^5.21.5",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^4.4.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/node": "^7.7.7",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.7.7",
    "@babel/runtime": "^7.11.2",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "ghooks": "^2.0.4",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "sequelize-cli": "^5.5.1"
  }
}
